
#include<stdio.h>
#include<string.h>
void sort(char a[][100])
{
	int x; char b[5];
	again:
	for(int i=0;i<5;i++)
	{
			if(strcmp(a[i],a[i+1])>0)
		    {
			//swap
            strcpy(b, a[i]);
            strcpy(a[i], a[i+1]);
            strcpy(a[i+1], b);
		    }	
	}
	for(int i=0;i<5;i++)
	{
		if(strcmp(a[i],a[i+1])>0)
         goto again;
	}
	for (int i=1; i<=5 ; i++)
	{
		printf("%s\n",a[i]);
	}
}
main()
{
	char a[][100]={"zebra", "giraffe", "tiger", "dragon", "monkey"};
	sort(a);
}
